{
  "buttonTypes": {
    "accept": {
      "selectors": [
        {
          "query": "#acceptBtn",
          "priority": 10
        },
        {
          "query": "button[id*=\"accept\"]",
          "priority": 8
        },
        {
          "query": ".accept-button",
          "priority": 9
        }
      ],
      "textPatterns": [
        {
          "pattern": "accept all cookies",
          "priority": 10
        },
        {
          "pattern": "accept all",
          "priority": 9
        },
        {
          "pattern": "accept",
          "priority": 5
        }
      ],
      "excludePatterns": [
        "settings",
        "preferences"
      ]
    },
    "reject": {
      "selectors": [
        {
          "query": "#rejectBtn",
          "priority": 10
        },
        {
          "query": "button[id*=\"reject\"]",
          "priority": 8
        }
      ],
      "textPatterns": [
        {
          "pattern": "reject all",
          "priority": 10
        },
        {
          "pattern": "necessary only",
          "priority": 9
        },
        {
          "pattern": "reject",
          "priority": 6
        }
      ]
    },
    "customize": {
      "selectors": [
        {
          "query": "#settingsBtn",
          "priority": 10
        },
        {
          "query": "button[id*=\"settings\"]",
          "priority": 8
        }
      ],
      "textPatterns": [
        {
          "pattern": "cookie settings",
          "priority": 10
        },
        {
          "pattern": "settings",
          "priority": 8
        }
      ]
    }
  },
  "checkboxTypes": {
