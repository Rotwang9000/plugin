  "dialogSelectors": {
    "selectors": [
      {
        "query": "#cookieDialog",
        "priority": 10
      },
      {
        "query": ".cookie-notice",
        "priority": 8
      }
    ],
    "textPatterns": [
      {
        "pattern": "cookie policy",
        "priority": 10
      },
      {
        "pattern": "cookie preferences",
        "priority": 9
      }
    ],
    "excludeSelectors": [
      "footer [class*=\"cookie\"]"
    ]
  },
  "regionDetection": {
    "eu": {
      "textPatterns": [
        {
          "pattern": "gdpr",
          "priority": 10
        },
        {
          "pattern": "european union",
          "priority": 10
        }
