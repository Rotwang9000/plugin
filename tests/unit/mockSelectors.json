    "advertising": {
      "selectors": [
        {
          "query": "#advertisingCheckbox",
          "priority": 10
        },
        {
          "query": "input[name*=\"advertising\"]",
          "priority": 8
        }
      ],
      "textPatterns": [
        {
          "pattern": "advertising cookies",
          "priority": 10
        },
        {
          "pattern": "marketing",
          "priority": 8
        }
      ]
    },
    "necessary": {
      "selectors": [
        {
          "query": "#necessaryCheckbox",
          "priority": 10
        },
        {
          "query": "input[name*=\"necessary\"]",
